  },
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.4.7",
    "bluebird": "^3.4.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "glob": "^7.0.3",
    "ignore": "^3.1.2",
    "mout": "^1.0.0",
    "progress": "^1.1.8",
